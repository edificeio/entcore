/**
 * Hook based on Bootstrap 5.x breakpoints
 * Respect mobile-first breakpoints
 * @returns $grid-breakpoints: (
  xs: 0,
  sm: 375px,
  md: 768px,
  lg: 1024px,
  xl: 1280px,
  xxl: 1400px,
) !default;
 */
export default function useBreakpoint(): {
    xs: boolean;
    sm: boolean;
    md: boolean;
    lg: boolean;
    xl: boolean;
    xxl: boolean;
};
