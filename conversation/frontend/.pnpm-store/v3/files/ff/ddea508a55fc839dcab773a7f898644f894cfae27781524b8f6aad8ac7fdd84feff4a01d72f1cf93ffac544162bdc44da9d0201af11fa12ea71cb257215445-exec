#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/node/app/node_modules/.pnpm/@nrwl+tao@19.6.0/node_modules/@nrwl/tao/node_modules:/home/node/app/node_modules/.pnpm/@nrwl+tao@19.6.0/node_modules/@nrwl/node_modules:/home/node/app/node_modules/.pnpm/@nrwl+tao@19.6.0/node_modules:/home/node/app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/node/app/node_modules/.pnpm/@nrwl+tao@19.6.0/node_modules/@nrwl/tao/node_modules:/home/node/app/node_modules/.pnpm/@nrwl+tao@19.6.0/node_modules/@nrwl/node_modules:/home/node/app/node_modules/.pnpm/@nrwl+tao@19.6.0/node_modules:/home/node/app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@nrwl+tao@19.6.0/node_modules/@nrwl/tao/index.js" "$@"
else
  exec node  "$basedir/../../../../../@nrwl+tao@19.6.0/node_modules/@nrwl/tao/index.js" "$@"
fi
