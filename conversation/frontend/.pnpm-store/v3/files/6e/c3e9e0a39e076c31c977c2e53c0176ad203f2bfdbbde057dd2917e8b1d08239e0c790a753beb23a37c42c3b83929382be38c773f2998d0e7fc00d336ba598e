import { ReactNode } from 'react';
declare const CardBody: {
    ({ children, space, flexDirection, }: {
        space?: "0" | "8" | "16" | null;
        flexDirection?: "row" | "column" | null;
        children: ReactNode;
    }): import("react/jsx-runtime").JSX.Element;
    displayName: string;
};
export default CardBody;
