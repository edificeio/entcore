          size: {
            width: application.stage.width,
            height: application.stage.height
          },
          scale: {
            x: application.stage.scale.x,
            y: application.stage.scale.y
          }
        }
      };
      return setHistory([...listSize(history), state]), await promise, callback.call(callback);
    }
  };
