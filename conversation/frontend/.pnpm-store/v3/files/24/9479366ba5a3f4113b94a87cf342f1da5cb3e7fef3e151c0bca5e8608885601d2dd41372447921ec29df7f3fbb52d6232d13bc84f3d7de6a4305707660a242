    files2 && addFiles([...files2]);
  }, handleDragging = (event) => {
    event.preventDefault(), event.stopPropagation(), setDragging(!0);
  }, handleDragLeave = (event) => {
    event.preventDefault(), event.stopPropagation(), setDragging(!1);
  };
