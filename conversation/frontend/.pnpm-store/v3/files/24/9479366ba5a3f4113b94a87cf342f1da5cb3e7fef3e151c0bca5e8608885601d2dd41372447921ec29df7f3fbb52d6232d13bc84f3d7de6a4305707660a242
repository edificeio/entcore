class StringUtils {
  static isLocalURL(str) {
    return str.length > 0 && str.charAt(0) === "/";
  }
  static startWithHttp(str) {
    return /^https?:\/\//gi.test(str);
  }
  static toCounter(counter) {
    return counter > 999 ? `${Number(counter / 1e3).toFixed(1)} k` : "" + counter;
  }
}
