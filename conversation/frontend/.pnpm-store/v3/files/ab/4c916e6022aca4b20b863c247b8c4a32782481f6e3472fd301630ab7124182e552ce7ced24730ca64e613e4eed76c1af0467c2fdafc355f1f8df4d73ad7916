import { jsx } from "react/jsx-runtime";
import { forwardRef } from "react";
import clsx from "clsx";
import { useTranslation } from "react-i18next";
import useEdificeIcons from "../../hooks/useEdificeIcons/useEdificeIcons.js";
const BreadcrumbNav = /* @__PURE__ */ forwardRef(({
  children,
  app,
  className
}, ref) => {
  const {
    t
  } = useTranslation(), {
    getIconClass
  } = useEdificeIcons(), classes = clsx("d-flex align-items-center mb-0", getIconClass(app), className), style = {
    "--edifice-breadcrumb-divider": "-"
  };
  return /* @__PURE__ */ jsx("nav", { ref, className: classes, "aria-label": t("breadcrumb"), style, children });
});
export {
  BreadcrumbNav as default
};
