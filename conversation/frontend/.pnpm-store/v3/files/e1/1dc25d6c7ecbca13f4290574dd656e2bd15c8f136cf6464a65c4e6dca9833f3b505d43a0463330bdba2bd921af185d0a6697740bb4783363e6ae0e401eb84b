import { IResource } from '@edifice.io/client';
interface UseThumbProps {
    isUpdating: boolean;
    selectedResource?: IResource;
}
export declare const useThumb: ({ isUpdating, selectedResource }: UseThumbProps) => {
    thumbnail: string | File | Blob;
    handleDeleteImage: () => void;
    handleUploadImage: (file: File | string) => void;
};
export {};
