{
  "name": "org.entcore~audience",
  "config": {
    "main":"org.entcore.audience.Audience",
    "port": 8091,
    "csrf-token": {{ csrfToken | default('true') }},
    "ssl": true,
    "mode": "prod",
    "auto-redeploy": false,
    "app-type" : "END_USER",
    "app-name" : "Audience",
    "app-address" : "/audience",
    "host": "https://{{ host }}",
    "sql": true,
    "db-schema": "audience",
    "publicConf": {
      "reaction-types" : {{ audienceReactionTypes | default('["REACTION_1","REACTION_2","REACTION_3","REACTION_4"]') }}
    },
    "broker": {
      "natsModules": {
        "communities": [ "announcement" ]
      }
    }
  }
}