{
  "name": "org.entcore~session",
  "worker": true,
  "config": {
    "slo": {{ sessionSlo | default('true') }},
    "slo-oidc-backchannel-logout": {{ openidSlo | default('false') }},
    "inactivity": {{ sessionInactivity | default('false') }},
    {% if sessionProlongedCookieTimeout is defined and sessionInactivity -%}
      "prolonged_session_timeout": {{ sessionProlongedCookieTimeout }}000,
    {% endif -%}
    "session_timeout": {{ sessionCookieTimeout | default('1800') }}000
  }
}